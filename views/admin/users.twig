<style>
    table, tr, th, td {
        border: 1px solid black;
        border-collapse: collapse;
        padding: 10px;
        text-align: center;
        max-width: 100vw;
    }
</style>

<table>
    <thead>
    <th>ID Murid</th>
    <th>No. Telefon</th>
    <th>Info Log Masuk</th>
    </thead>
    <tbody>
    {% for user in users %}
        <tr>
            <td>{{ user['idMurid'] }}</td>
            <td>{{ user['noTel'] }}</td>
            <td>{{ user['infoLogMasuk'] }}</td>
        </tr>
    {% endfor %}
    </tbody>
</table>